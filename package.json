{
	"name": "catuccino",
	"version": "0.1.12",
	"private": false,
	"description": "A React.JS ui kit based on ðŸ˜¸ catppuccin - Soothing pastel theme for the high-spirited!",
	"keywords": [
		"catuccino",
		"catppuccin",
		"ui-kit",
		"ui",
		"component library"
	],
	"repository": "didair/catuccino",
	"license": "MIT",
	"author": "Andreas EkstrÃ¶m <didair@msn.com>",
	"source": "src/index.js",
	"main": "./dist/catuccino.js",
	"jsnext:main": "./dist/catuccino.modern.js",
	"module": "./dist/catuccino.module.js",
	"syntax": {
		"esmodules": "./dist/catuccino.modern.js"
	},
	"exports": {
		".": {
			"browser": "./dist/catuccino.module.js",
			"import": "./dist/catuccino.module.js",
			"require": "./dist/catuccino.js"
		}
	},
	"files": [
		"dist",
		"dist/*.css"
	],
	"scripts": {
		"build": "microbundle --no-compress -f cjs,esm,modern --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react",
		"build-storybook": "build-storybook -s public",
		"start": "start-storybook -p 6006 -s public",
		"watch": "microbundle watch --jsx React.createElement"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"eqeqeq": "off",
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"peerDependencies": {
		"react": "^17.0.2 || ^18.0.0",
		"react-dom": "^17.0.2 || ^18.0.0"
	},
	"dependencies": {
		"@catppuccin/palette": "^0.1.4",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^13.5.0",
		"classnames": "^2.3.1",
		"prop-types": "^15.8.1"
	},
	"devDependencies": {
		"@storybook/addon-actions": "^6.5.9",
		"@storybook/addon-essentials": "^6.5.9",
		"@storybook/addon-interactions": "^6.5.9",
		"@storybook/addon-links": "^6.5.9",
		"@storybook/builder-webpack5": "^6.5.9",
		"@storybook/manager-webpack5": "^6.5.9",
		"@storybook/node-logger": "^6.5.9",
		"@storybook/preset-create-react-app": "^4.1.2",
		"@storybook/react": "^6.5.9",
		"@storybook/testing-library": "^0.0.13",
		"autoprefixer": "^10.4.7",
		"babel-plugin-named-exports-order": "^0.0.2",
		"microbundle": "^0.15.0",
		"postcss": "^8.4.14",
		"postcss-loader": "^7.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tailwindcss": "^0.0.0-insiders.60a0ae2",
		"webpack": "^5.73.0"
	},
	"overrides": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
